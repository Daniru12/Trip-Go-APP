<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.daniru.tripgo.fragment.ExploreFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_explore" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.daniru.tripgo.fragment.SavedFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_saved" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.daniru.tripgo.fragment.NearFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_near" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.daniru.tripgo.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.daniru.tripgo.fragment.HomeFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_home" />
</navigation>